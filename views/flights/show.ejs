<%- include('../partials/header') %>
    <h2>Flight Details</h2>
    <table>
        <thead>
            <tr>
                <th>Airline Name</th>
                <th>Airport</th>
                <th>Flight Number</th>
                <th>Departs</th>
                <th>Arrivals</th>
            </tr>
        </thead>
        <tbody>
                <tr>
                    <td>
                        <%= flight.airline %>
                    </td>
                    <td>
                        <%= flight.airport %>
                    </td>
                    <td>
                        <%= flight.flightNo %>
                    </td>
                    <td>
                        <%= flight.departs %>
                    </td>
                    <td>
                        <%= flight.arrival %>
                    </td>
                </tr>
        </tbody>
    </table>
    <%- include('../partials/footer') %>